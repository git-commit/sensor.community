{
  "name": "sensor.community",
  "description": "new shiny website for dust and noise measuring project sensor.community based on sapper and svelte",
  "version": "0.8.5",
  "packageManager": "yarn@3.1.1",
  "scripts": {
    "dev": "sapper dev --ext '.svelte' start:dev autobuild watch:tailwind",
    "watch:tailwind": "postcss static/tailwind.css -o static/index.css -w",
    "build:tailwind": "NODE_ENV=production postcss static/tailwind.css -o static/index.css",
    "export": "NODE_ENV=production postcss static/tailwind.css -o static/index.css && sapper export"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@polka/send": "^1.0.0-next.15",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.0.1",
    "@sindresorhus/slugify": "^1.1.2",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/typography": "^0.5.1",
    "autoprefixer": "^10.4.2",
    "compression": "^1.7.4",
    "cssnano": "^5.0.16",
    "golden-fleece": "^1.0.9",
    "highlight.js": "^11.4.0",
    "i18next": "^21.6.10",
    "marked": "^2.1.3",
    "polka": "1.0.0-next.22",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "rollup": "^2.66.1",
    "rollup-plugin-babel": "^5.0.0-alpha.2",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.29.3",
    "sirv": "^1.0.19",
    "svelte-i18n": "^3.3.13",
    "tailwindcss": "^3.0.18"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "svelte": "^3.46.3"
  }
}
